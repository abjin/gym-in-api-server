{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/s3/(.*)": "<rootDir>/../libs/s3/src/$1",
    "@libs/s3": "<rootDir>/../libs/s3/src",
    "@libs/date/(.*)": "<rootDir>/../libs/date/src/$1",
    "@libs/date": "<rootDir>/../libs/date/src"
  }
}